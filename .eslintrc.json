{
  "env": {
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "indent": ["error", 4],
    "no-console": "off"
  }
}
